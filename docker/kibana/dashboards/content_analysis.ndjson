{
  "version": "7.17.0",
  "objects": [
    {
      "id": "content-overview",
      "type": "dashboard",
      "attributes": {
        "title": "Content Overview",
        "description": "Overview of crawled content statistics",
        "hits": 0,
        "panels": [
          {
            "id": "total-pages",
            "type": "visualization",
            "attributes": {
              "title": "Total Pages",
              "visState": {
                "type": "metric",
                "aggs": [
                  {
                    "id": "1",
                    "type": "count",
                    "schema": "metric"
                  }
                ]
              }
            }
          },
          {
            "id": "content-length-distribution",
            "type": "visualization",
            "attributes": {
              "title": "Content Length Distribution",
              "visState": {
                "type": "histogram",
                "aggs": [
                  {
                    "id": "1",
                    "type": "histogram",
                    "field": "content_stats.text_length",
                    "interval": 1000
                  }
                ]
              }
            }
          },
          {
            "id": "sections-per-page",
            "type": "visualization",
            "attributes": {
              "title": "Sections per Page",
              "visState": {
                "type": "pie",
                "aggs": [
                  {
                    "id": "1",
                    "type": "terms",
                    "field": "content_stats.section_count",
                    "size": 10
                  }
                ]
              }
            }
          }
        ]
      }
    },
    {
      "id": "image-analysis",
      "type": "dashboard",
      "attributes": {
        "title": "Image Analysis",
        "description": "Analysis of image usage across pages",
        "hits": 0,
        "panels": [
          {
            "id": "images-per-page",
            "type": "visualization",
            "attributes": {
              "title": "Images per Page",
              "visState": {
                "type": "histogram",
                "aggs": [
                  {
                    "id": "1",
                    "type": "histogram",
                    "field": "content_stats.image_count",
                    "interval": 1
                  }
                ]
              }
            }
          },
          {
            "id": "missing-alt-text",
            "type": "visualization",
            "attributes": {
              "title": "Images Missing Alt Text",
              "visState": {
                "type": "metric",
                "aggs": [
                  {
                    "id": "1",
                    "type": "nested",
                    "path": "images",
                    "aggs": [
                      {
                        "id": "2",
                        "type": "filter",
                        "filter": {
                          "bool": {
                            "must_not": [
                              {
                                "exists": {
                                  "field": "images.alt"
                                }
                              }
                            ]
                          }
                        }
                      }
                    ]
                  }
                ]
              }
            }
          }
        ]
      }
    },
    {
      "id": "link-analysis",
      "type": "dashboard",
      "attributes": {
        "title": "Link Analysis",
        "description": "Analysis of internal and external links",
        "hits": 0,
        "panels": [
          {
            "id": "link-distribution",
            "type": "visualization",
            "attributes": {
              "title": "Link Distribution",
              "visState": {
                "type": "pie",
                "aggs": [
                  {
                    "id": "1",
                    "type": "terms",
                    "field": "content_stats.internal_link_count",
                    "title": "Internal Links"
                  },
                  {
                    "id": "2",
                    "type": "terms",
                    "field": "content_stats.external_link_count",
                    "title": "External Links"
                  }
                ]
              }
            }
          },
          {
            "id": "most-linked-pages",
            "type": "visualization",
            "attributes": {
              "title": "Most Linked Pages",
              "visState": {
                "type": "table",
                "aggs": [
                  {
                    "id": "1",
                    "type": "terms",
                    "field": "links.internal",
                    "size": 20
                  }
                ]
              }
            }
          }
        ]
      }
    }
  ]
}